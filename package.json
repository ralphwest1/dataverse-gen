{
  "name": "@dometag/dataverse-gen",
  "version": "1.0.17",
  "description": "Earlybound types generator for dataverse-ify",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "dataverse-gen": "lib/index.js"
  },
  "scripts": {
    "start": "tsc & node lib/index.js -s org.crm11.dynamics.com",
    "start-ts": "ts-node -T src/index",
    "start-help": "tsc & node lib/index.js help",
    "start-init": "tsc & node lib/index.js init",
    "start-eject": "tsc & node lib/index.js eject",
    "build": "tsc",
    "test": "tsc & jest --passWithNoTests",
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "genversion --es6 --semi src/version.ts & git add . & git commit -m \"Version bump\" & git push",
    "genversion": "genversion",
    "prepublishOnly": "npm version patch"
  },
  "author": "Scott Durow",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.0",
    "dataverse-ify": "^1.0.9",
    "ejs": "^3.1.5",
    "enquirer": "^2.3.6",
    "fs-extra": "^9.0.1",
    "lodash.merge": "^4.6.2",
    "minimist": "^1.2.5",
    "node-fetch": "^2.6.1",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/config": "0.0.36",
    "@types/cryptr": "^4.0.1",
    "@types/ejs": "^3.0.5",
    "@types/fs-extra": "^8.1.1",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^24.9.1",
    "@types/lodash.merge": "^4.6.6",
    "@types/minimist": "^1.2.1",
    "@types/node": "^13.13.39",
    "@types/node-fetch": "^2.5.7",
    "@types/xrm": "^9.0.33",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "genversion": "^2.2.1",
    "jest": "^24.9.0",
    "jest-environment-jsdom-fourteen": "^0.1.0",
    "prettier": "^1.18.2",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.7"
  }
}
